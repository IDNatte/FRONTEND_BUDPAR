{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run build:tailwind && rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --single --no-clear --port 3000",
    "dev:tailwind": "postcss src/App.css -o public/build/vendor.css -w",
    "build:tailwind": "cross-env NODE_ENV=production postcss src/App.css -o public/build/vendor.css"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "semver": "^7.3.7",
    "svelte": "^3.0.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "leaflet": "1.7.1",
    "lodash": "^4.17.21",
    "sirv-cli": "^2.0.0",
    "svelte-carousel": "^1.0.17-rc4",
    "svelte-lazy": "^1.1.0",
    "svelte-spa-router": "^3.2.0",
    "svelte-tabs": "^1.1.0"
  }
}