{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run build:tailwind && rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"yarn run dev:tailwind\"",
    "start": "sirv public --single --no-clear --port 3000",
    "dev:tailwind": "postcss src/App.css -o public/build/vendor.css -w",
    "build:tailwind": "cross-env NODE_ENV=production postcss src/App.css -o public/build/vendor.css"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0",
    "svelte-spa-router": "^3.2.0"
  }
}
